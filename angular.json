{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angcliapp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./extra-webpack.config.js",
              "mergeRules": {
                "externals": "replace"
              }
            },
            "outputPath": "dist/angcliapp/browser",
            "index":{ "input": "src/index.html"},
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/CNAME", "src/assets/"],
            "styles": ["node_modules/bootstrap/scss/bootstrap.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "100kb",
                  "maximumError": "150kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angcliapp:build:production"
            },
            "development": {
              "browserTarget": "angcliapp:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angcliapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.json",
            "inlineStyleLanguage": "scss",
            "assets": [],
            "styles": [],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-builders/custom-webpack:server",
          "options": {
            "outputPath": "dist/angcliapp/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/CNAME", "src/assets/"],
            "customWebpackConfig": {
              "path": "./extra-webpack.config.js",
              "mergeRules": { "module": { "rules": "prepend" } },
              "replaceDuplicatePlugins": true
            }
          },
          "configurations": {
            "production": {
              "outputHashing": "media"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "angcliapp:build:development",
              "serverTarget": "angcliapp:server:development"
            },
            "production": {
              "browserTarget": "angcliapp:build:production",
              "serverTarget": "angcliapp:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": [
              "/price/1",
              "/price/2",
              "/price/3",
              "/price/desc/1",
              "/price/desc/2",
              "/price/desc/3",
              "/power/1",
              "/power/2",
              "/power/3",
              "/power/desc/1",
              "/power/desc/2",
              "/power/desc/3",
              "/cc/1",
              "/cc/2",
              "/cc/3",
              "/cc/desc/1",
              "/cc/desc/2",
              "/cc/desc/3",
              "/about"

            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "angcliapp:build:production",
              "serverTarget": "angcliapp:server:production"
            },
            "development": {
              "browserTarget": "angcliapp:build:development",
              "serverTarget": "angcliapp:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "cli": {
    "analytics": "610db447-4bf3-4aab-85eb-9d37dc58bf93"
  }
}
